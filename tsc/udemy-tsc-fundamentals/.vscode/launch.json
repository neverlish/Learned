{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Typescript debug",
      "program": "${workspaceRoot}/app.ts",
      "outFiles": ["${workspaceRoot}/**/*.js}"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "address": "localhost",
      "port": 5858,
      "outFiles": []
    }
  ]
}
